              <a href="/messages"><i>Messages</i></a>
          </div>
          <div class="uk-navbar-item">
              <a href="/game"><i>Game</i></a>
          </div>
        </div>
        <div class="uk-navbar-right">
          <div class="uk-navbar-item">
              <a href="/signin"><i>Sign In</i></a>
          </div>
          <div class="uk-navbar-item">
              <a href="/signup"><i>Sign Up</i></a>
          </div>
